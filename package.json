    "@stripe/stripe-js": "^2.4.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.0",
    "cookie": "^0.6.0",
    "framer-motion": "^11.0.8",
    "graphql": "^16.8.1",
    "ioredis": "^5.3.2",
    "jose": "^5.2.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-hook-form": "^7.50.1",
    "react-hot-toast": "^2.4.1",
