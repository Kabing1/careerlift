    "postbuild": "echo Build completed successfully",
    "test": "vitest",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "validate": "npm run lint && npm run typecheck"
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
